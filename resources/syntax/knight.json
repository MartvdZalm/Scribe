{
  "language": "Knight",
  "extensions": [".kn", ".knight"],
  "keywords": ["func", "if", "else", "ret"],
  "types": ["int", "float", "string", "bool"],
  "literals": ["true", "false", "null"],
  "comments": {
    "line": "//",
    "block": {
      "start": "/*",
      "end": "*/"
    }
  },
  "strings": ["\"", "'"],
  "numbers": {
    "regex": "\\b[0-9]+\\b"
  }
}
